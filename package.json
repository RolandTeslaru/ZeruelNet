{
  "name": "zeruel-net",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/GatewayService",
    "packages/DataScraper/*",
    "packages/SharedUI",
    "packages/types",
    "packages/ZeruelDashboard/*"
  ],
  "scripts": {
    "gateway:start": "npm run start --workspace=@zeruel/gateway-service",
    "gateway:dev": "npm run dev --workspace=@zeruel/gateway-service",
    "scraper-service:start": "npm run start --workspace=@zeruel/scraper-service",
    "scraper-service:dev": "npm run dev:server --workspace=@zeruel/scraper-service",
    "scraper-ui:start": "npm run start --workspace=@zeruel/scraper-ui",
    "scraper-ui:dev": "npm run dev --workspace=@zeruel/scraper-ui",
    "types:build": "npm run build --workspace=@zeruel/scraper-types",
    "encirhcment:dev": "source packages/DataEnrichment/venv/bin/activate && python packages/DataEnrichment/worker.py",
    "dashboard-ui:start": "npm run start --workspace=@zeruel/dashboard-ui",
    "dashboard-ui:dev": "npm run dev --workspace=@zeruel/dashboard-ui",
    "dashboard-serivce:start": "npm run start --workspace=@zeruel/dashboard-service",
    "dashboard-service:dev": "npm run dev --workspace=@zeruel/dashboard-service"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "dotenv-flow": "^4.1.0",
    "react18-json-view": "^0.2.9"
  }
}
